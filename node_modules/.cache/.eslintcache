 Recettes;","/home/loic/Documents/old/ubuntu/tpundeux/src/components/menu/Menu.jsx",["54","55","56","57","58","59","60","61","62"],[],"import React from 'react';\nimport css from './Menu.module.css';\nimport { BrowserRouter, Routes, Route, NavLink } from \"react-router-dom\";\nimport Recettes from '../Recettes.js';\nimport Home from '../../App.js';\nimport Blog from '../Blog.js';\n\nconst Menu = () => {\n    \n    let activeStyle = {\n        textDecoration: \"underline\",\n    };\n    \n    let activeClassName = \"underline\";\n    \n    return (\n        <nav>\n            <div style={{ margin: '10px' }}>\n                <NavLink to=\"/\" style={({ isActive }) => ({ \n                    color: isActive ? 'greenyellow' : 'white' })}>\n                    Accueil\n                </NavLink>\n            </div>\n            <div style={{ margin: '10px' }}>\n                <NavLink to=\"/recettes\" style={({ isActive }) => ({ \n                    color: isActive ? 'greenyellow' : 'white' })}>\n                    Recettes\n                </NavLink>\n            </div>\n            <div style={{ margin: '10px' }}>\n                <NavLink to=\"/blog\" style={({ isActive }) => ({ \n                    color: isActive ? 'greenyellow' : 'white' })}>\n                    Blog\n                </NavLink>\n            </div>\n        </nav>\n    );\n};\n\nexport default Menu;","/home/loic/Documents/old/ubuntu/tpundeux/src/components/Filter.js",[],[],"/home/loic/Documents/old/ubuntu/tpundeux/src/components/Article.js",["63"],[],"/home/loic/Documents/old/ubuntu/tpundeux/src/components/Card.jsx",["64","65","66"],[],"import React from 'react';\nimport { useState, useEffect } from \"react\";\nimport './Card.css';\n\nconst Card = (props) => {\n    \n    return (\n        <div className='cardContainer'>\n          <h1>{props.props['strMeal']}</h1>\n          <h3>{props.props['strArea']}</h3>\n          <img style={{width: 15 + 'rem'}} src={props.props['strMealThumb']}></img>\